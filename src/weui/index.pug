            .page__category.js_categoryInner
              .weui-cells.page__category-content
                a.weui-cell.weui-cell_access.js_item(data-id="button", href="../../dist/weui/widget/form/button/index.html")
                  .weui-cell__bd
                    p Button
                                           
                  .weui-cell__ft
                                    
